{
    "name": "ecommit/csv-table-generator",
    "description": "Create a CSV file with PHP array.",
    "keywords": ["csv", "array", "table"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hubert Lecorche",
            "email": "contact@e-commit.fr"
        }
    ],
    "autoload": {
        "psr-4": { "Ecommit\\CsvTableGenerator\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Ecommit\\CsvTableGenerator\\Tests\\": "tests/" }
    },
    "require": {
        "php": "^7.4|^8.0",
        "symfony/options-resolver": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
