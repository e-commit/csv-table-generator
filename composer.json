{
    "name": "ecommit/csv-table-generator",
    "description": "Create a CSV file with PHP array.",
    "keywords": ["csv", "array", "table"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hubert Lecorche",
            "email": "contact@e-commit.fr"
        }
    ],
    "autoload": {
        "psr-4": { "Ecommit\\CsvTableGenerator\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Ecommit\\CsvTableGenerator\\Tests\\": "tests/" }
    },
    "require": {
        "php": "^8.1",
        "symfony/options-resolver": "^6.4|^7.3|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "maglnet/composer-require-checker": "*",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
